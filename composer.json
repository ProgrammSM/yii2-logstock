{
    "name": "pastuhov/yii2-logstock",
    "description": "The testing extension for the Yii framework",
    "keywords": ["yii2", "testing", "debug", "debugger"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/pastuhov/yii2-logstock/issues",
        "source": "https://github.com/pastuhov/yii2-logstock"
    },
    "authors": [
        {
            "name": "Pastukhov Kirill",
            "email": "kirill@pastukhov.su",
            "homepage": "https://github.com/pastuhov/",
            "role": "Developer"

        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.4",
        "yiisoft/yii2-debug": "~2.0.6"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "2.0.*",
        "codeception/codeception": "2.2.*",
        "codeception/specify": "0.4.*",
        "codeception/verify": "0.2.*"
    },
    "autoload": {
        "psr-4": {
            "pastuhov\\logstock\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pastuhov\\logstock\\tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/codecept run"
    }
}
